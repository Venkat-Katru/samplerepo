<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="Properties"
    sling:resourceType="cq/gui/components/authoring/dialog">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
        <items jcr:primaryType="nt:unstructured">
            <column
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/container">
                <items jcr:primaryType="nt:unstructured">
                    <tabs
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="/libs/granite/ui/components/coral/foundation/tabs">
                        <items jcr:primaryType="nt:unstructured">
                            <Header
                                jcr:primaryType="nt:unstructured"
                                jcr:title="Header"
                                sling:resourceType="granite/ui/components/coral/foundation/container">
                                <items jcr:primaryType="nt:unstructured">
                                    <Logo_x0020_Path
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="/libs/granite/ui/components/coral/foundation/form/pathfield"
                                        fieldLabel="Logo Path"
                                        name="./logoPath"/>
                                    <Logo_x0020_Mobile_x0020_Image
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="/libs/granite/ui/components/coral/foundation/form/pathfield"
                                        fieldLabel="Logo Mobile Image path"
                                        name="./logoMobileImage"/>
                                    <Logo_x0020_Link
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="/libs/granite/ui/components/coral/foundation/form/pathfield"
                                        fieldLabel="Logo Link Path"
                                        name="./logoLink"/>
                                    <Enable_x0020_Switch
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="/libs/granite/ui/components/coral/foundation/form/checkbox"
                                        fieldLabel="Enable Switch"
                                        name="./enableSwitch"
                                        text="Enable Switch"
                                        value="{Boolean}true"/>
                                </items>
                            </Header>
                            <Header_x0020_Links
                                jcr:primaryType="nt:unstructured"
                                jcr:title="Header-Links"
                                sling:resourceType="granite/ui/components/coral/foundation/container">
                                <items jcr:primaryType="nt:unstructured">
                                    <multifield
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="/libs/granite/ui/components/coral/foundation/form/multifield"
                                        composite="{Boolean}true"
                                        fieldLabel="Multifield">
                                        <field
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/container"
                                            name="./multifield">
                                            <items jcr:primaryType="nt:unstructured">
                                                <Name
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="/libs/granite/ui/components/coral/foundation/form/textfield"
                                                    fieldLabel="Text"
                                                    name="./name"/>
                                                <Image
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="/libs/granite/ui/components/coral/foundation/form/pathfield"
                                                    fieldLabel="Image Path"
                                                    name="./image"/>
                                            </items>
                                        </field>
                                    </multifield>
                                </items>
                            </Header_x0020_Links>
                            <Sidebar-Navigation
                                jcr:primaryType="nt:unstructured"
                                jcr:title="Sidebar Navigation"
                                sling:resourceType="granite/ui/components/coral/foundation/container">
                                <items jcr:primaryType="nt:unstructured">
                                    <multifield
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="/libs/granite/ui/components/coral/foundation/form/multifield"
                                        composite="{Boolean}true"
                                        fieldLabel="Multifield2">
                                        <field
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/container"
                                            name="./multifield2">
                                            <items jcr:primaryType="nt:unstructured">
                                                <Desktop_x0020_Icon
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="/libs/granite/ui/components/coral/foundation/form/pathfield"
                                                    fieldLabel="Desktop Icon"
                                                    name="./desktopicon"/>
                                                <Mobile_x0020_Icon
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="/libs/granite/ui/components/coral/foundation/form/pathfield"
                                                    fieldLabel="Mobile Icon"
                                                    name="./mobileicon"/>
                                                <nestedmultifield
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="/libs/granite/ui/components/coral/foundation/form/multifield"
                                                    composite="{Boolean}true"
                                                    fieldLabel="Nested Multifield">
                                                    <field
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/container"
                                                        name="./nestedmultifield">
                                                        <items jcr:primaryType="nt:unstructured">
                                                            <Navigation_x0020_Url
                                                                jcr:primaryType="nt:unstructured"
                                                                sling:resourceType="/libs/granite/ui/components/coral/foundation/form/pathfield"
                                                                fieldLabel="Navigation URL"
                                                                name="./navigationurl"/>
                                                        </items>
                                                    </field>
                                                </nestedmultifield>
                                            </items>
                                        </field>
                                    </multifield>
                                </items>
                            </Sidebar-Navigation>
                            <Region
                                jcr:primaryType="nt:unstructured"
                                jcr:title="Country"
                                sling:resourceType="granite/ui/components/coral/foundation/container">
                                <items jcr:primaryType="nt:unstructured">
                                    <Country
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="/libs/granite/ui/components/coral/foundation/form/select"
                                        fieldLabel="select country"
                                        name="./countries">
                                        <items jcr:primaryType="nt:unstructured">
                                            <India
                                                jcr:primaryType="nt:unstructured"
                                                text="India"
                                                value="ind"/>
                                            <usa
                                                jcr:primaryType="nt:unstructured"
                                                text="USA"
                                                value="usa"/>
                                        </items>
                                    </Country>
                                </items>
                            </Region>
                        </items>
                    </tabs>
                </items>
            </column>
        </items>
    </content>
</jcr:root>
