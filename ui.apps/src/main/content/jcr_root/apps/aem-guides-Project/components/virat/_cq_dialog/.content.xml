<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    sling:resourceType="cq/gui/components/authoring/dialog">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/coral/foundation/container">
        <items jcr:primaryType="nt:unstructured">
            <articledescription
                jcr:primaryType="nt:unstructured"
                sling:resourceType="/libs/granite/ui/components/coral/foundation/form/textarea"
                fieldLabel="description"
                name="./description"/>
            <articleimage
                jcr:primaryType="nt:unstructured"
                sling:resourceType="/libs/granite/ui/components/coral/foundation/form/pathbrowser"
                fieldLabel="select image"
                name="./image"/>
            <checkbox
                jcr:primaryType="nt:unstructured"
                sling:resourceType="/libs/granite/ui/components/coral/foundation/form/checkbox"
                fieldLabel="checkbox"
                fieldLable="articleNumber"
                name="./articleCheckbox"
                value="{Boolean}true"/>
            <articledate
                jcr:primaryType="nt:unstructured"
                sling:resourceType="/libs/granite/ui/components/coral/foundation/form/datepicker"
                fieldLabel="date"
                name="./date"/>
            <types
                jcr:primaryType="nt:unstructured"
                sling:resourceType="/libs/granite/ui/components/coral/foundation/form/select"
                name="./articleType">
                <items
                    jcr:primaryType="nt:unstructured"
                    fieldlabel="select headers">
                    <h2
                        jcr:primaryType="nt:unstructured"
                        text="heading2"
                        value="h2"/>
                    <h1
                        jcr:primaryType="nt:unstructured"
                        text="heading 1"
                        value="h1"/>
                </items>
            </types>
            <number
                jcr:primaryType="nt:unstructured"
                sling:resourceType="/libs/granite/ui/components/coral/foundation/form/numberfield"
                fieldLabel="number"
                name="./num"/>
            <textfield
                jcr:primaryType="nt:unstructured"
                sling:resourceType="/libs/granite/ui/components/coral/foundation/form/textfield"
                fieldLabel="text"
                name="./textfield"/>
            <multifield
                jcr:primaryType="nt:unstructured"
                sling:resourceType="/libs/granite/ui/components/coral/foundation/form/multifield"
                fieldLabel="text">
                <field
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="/libs/granite/ui/components/coral/foundation/form/textfield"
                    fieldLabel="multifield"
                    name="./mfield"/>
            </multifield>
            <nextedmultifield
                jcr:primaryType="nt:unstructured"
                sling:resourceType="/libs/granite/ui/components/coral/foundation/form/multifield"
                composite="{Boolean}true"
                fieldLabel="nextedmultifield">
                <field
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="granite/ui/components/coral/foundation/container"
                    name="./nextedmultifield">
                    <items jcr:primaryType="nt:unstructured">
                        <textfield
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="/libs/granite/ui/components/coral/foundation/form/textfield"
                            fieldLabel="text"
                            name="./textfield"/>
                        <articleimage
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="/libs/granite/ui/components/coral/foundation/form/pathfield"
                            fieldLabel="select image"
                            name="./image"/>
                    </items>
                </field>
            </nextedmultifield>
            <tabs
                jcr:primaryType="nt:unstructured"
                sling:resourceType="/libs/granite/ui/components/coral/foundation/tabs">
                <items jcr:primaryType="nt:unstructured">
                    <article-basic
                        jcr:primaryType="nt:unstructured"
                        jcr:title="basic"
                        sling:resourceType="granite/ui/components/coral/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            <articledate
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="/libs/granite/ui/components/coral/foundation/form/datepicker"
                                fieldLabel="date"
                                name="./date"/>
                            <number
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="/libs/granite/ui/components/coral/foundation/form/numberfield"
                                fieldLabel="number"
                                name="./num"/>
                            <textfield
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="/libs/granite/ui/components/coral/foundation/form/textfield"
                                fieldLabel="text"
                                name="./textfield"/>
                        </items>
                    </article-basic>
                    <article-advanced
                        jcr:primaryType="nt:unstructured"
                        jcr:title="advance"
                        sling:resourceType="granite/ui/components/coral/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            <articledate
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="/libs/granite/ui/components/coral/foundation/form/datepicker"
                                fieldLabel="date"
                                name="./date"/>
                            <number
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="/libs/granite/ui/components/coral/foundation/form/numberfield"
                                fieldLabel="number"
                                name="./num"/>
                            <textfield
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="/libs/granite/ui/components/coral/foundation/form/textfield"
                                fieldLabel="text"
                                name="./textfield"/>
                        </items>
                    </article-advanced>
                </items>
            </tabs>
        </items>
    </content>
</jcr:root>
